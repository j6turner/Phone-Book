<h1>Phone Book</h1>
<hr />
<h3>Add a contact:</h3>
<br />

<form action="/phonebook" method="post">

  <label for='name'><ul>Name:  </ul></label>
  <input id='name' name='name' type='text'>
  <br />
  <br />

  <label for='cell'><ul>Cell Number:  </ul></label>
  <input id='cell' name='cell' type='text'>
  <br />
  <br />

  <label for='home'><ul>Home Number:  </ul></label>
  <input id='home' name='home' type='text'>
  <br />
  <br />

  <label for='work'><ul>Work Number:  </ul></label>
  <input id='work' name='work' type='text'>
  <br />
  <br />

  <button class="btn btn-success">Add</button>

</form>
<br />
<br/>

<form action="/clear" method="post">
  <button type="submit" class="btn btn-danger">Clear</button>
</form>
